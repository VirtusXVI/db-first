ID - Number - BIGINT - PRIMARY_KEY(NOTNULL, UNIQUE, AUTOINCREMENT, INDEX)

NOME - String - VARCHAR(30) - NOTNULL

IMMAGINE - DEFAULT(placeholder) - NULL

CHILOMETRAGGIO - Number - MEDIUM INT - NULL

TIPO DI CAMBIO - String - VARCHAR(15) - NULL

ANNO - Date - YEAR - NULL

CARBURANTE - String - VARCHAR(15) - NULL

POTENZA - String / Number - SMALL INT - NULL

VENDITORE - String - VARCHAR(30) - NOTNULL

CARROZZERIA - String - VARCHAR(20) - NULL

TRAZIONE - String - VARCHAR(20) - NULL

POSTI - Number - TINY INT - NULL

PORTE - Number - TINY INT - NULL

DESCRIZIONE - String - TEXT - NULL